{
    "name": "anthonyrave/riot-api-connector",
    "version": "v0.4.0",
    "description": "A Laravel package to play with Riot Games' API.",
    "keywords": ["Laravel", "API", "Riot Games", "League Of Legends"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/anthonyrave/riot-api-connector/issues",
        "source": "https://github.com/anthonyrave/riot-api-connector"
    },
    "authors": [
        {
            "name": "Anthony Rave",
            "email": "anthony.rave@icloud.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.5",
        "laravel/framework": "^10.4",
        "spatie/laravel-translatable": "^6.4"
    },
    "require-dev": {
        "laravel/pint": "^1.7",
        "orchestra/testbench": "^8.3",
        "pestphp/pest": "^2.4",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "RiotApiConnector\\": "src/",
            "Database\\Factories\\": "database/factories",
            "Database\\Seeders\\": "database/seeders"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RiotApiConnector\\RiotApiConnectorServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
